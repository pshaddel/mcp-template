{
	"include": ["src/**/*"],
	"exclude": ["node_modules"],
	"compilerOptions": {
		"target": "ES2024",
		"module": "Node16",
		"moduleResolution": "Node16",
		"outDir": "./build",
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noImplicitAny": false,
		"incremental": true,
	}
}
